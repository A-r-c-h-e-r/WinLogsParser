<Window x:Class="WinLogsParser.View.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WinLogsParser" xmlns:viewmodel="clr-namespace:WinLogsParser.ViewModel"
        mc:Ignorable="d"
        Title="MainWindow" 
        Height="450" Width="800"
        MinHeight="360" MinWidth="260">

    <Window.DataContext>
        <viewmodel:ViewModelMainWindow/>
    </Window.DataContext>

    <Grid>
        <Frame NavigationUIVisibility="Hidden"
               Source="{Binding Path=ObjSourcePages.source, Mode=TwoWay}"/>
    </Grid>
</Window>
